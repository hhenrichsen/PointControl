{
  "private": true,
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "prebuild": "rimraf dist/*",
    "run": "tsc && node dist/main.js | bunyan",
    "dev": "nodemon --legacy-watch --watch src --exec \"npm run run\" -e ts --polling-interval 2500",
    "typeorm": "typeorm-ts-node-commonjs",
    "generate": "typeorm-ts-node-commonjs migration:generate ./src/migrations/autogenerated -d ./datasource.js",
    "format": "eslint --fix src && prettier --write src",
    "format:check": "eslint src && prettier --check src",
    "migrate": "typeorm-ts-node-commonjs migration:run -d ./datasource.js"
  },
  "keywords": [],
  "dependencies": {
    "@koa/router": "^12.0.0",
    "@types/jest": "^29.5.1",
    "argon2": "^0.30.3",
    "bunyan": "^1.8.15",
    "dotenv": "^16.0.3",
    "glob": "^8.1.0",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-jwt": "^4.0.4",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.15"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^18.16.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "jest-junit": "^16.0.0",
    "nodemon": "^2.0.22",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
